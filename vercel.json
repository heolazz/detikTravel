{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      },
      {
        "src": "resources/views/**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "public/**/*.js",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*.png",
        "use": "@vercel/static"
      },
      {
        "src": "public/**/*.jpg",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ]
  }
  